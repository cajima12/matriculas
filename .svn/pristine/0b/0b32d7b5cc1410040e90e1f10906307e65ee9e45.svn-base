package org.ayuncordoba.eseg.modelo;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.ManyToOne;
import javax.persistence.Table;


/**
 * The persistent class for the "ROLES" database table.
 * 
 */
@Entity
@Table(name="boe_tipo_texto")
public class BoeTipoTexto implements Serializable {

	private static final long serialVersionUID = -6157465538888297733L;

    @Id
    @GeneratedValue(strategy = GenerationType.AUTO)
	private Long id;
	
    @ManyToOne
    @JoinColumn(name = "DIR3_ID")
	private Dir3 dir3;

	@Lob
	private String cabecera;

	@Lob
	private String pie;
	
	private String tipo;
	
	
	public BoeTipoTexto() {
	}

	public Long getId() {
		return this.id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Dir3 getDir3() {
		return dir3;
	}

	public void setDir3(Dir3 dir3) {
		this.dir3 = dir3;
	}

	public String getCabecera() {
		return cabecera;
	}

	public void setCabecera(String cabecera) {
		this.cabecera = cabecera;
	}

	public String getPie() {
		return pie;
	}

	public void setPie(String pie) {
		this.pie = pie;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

}