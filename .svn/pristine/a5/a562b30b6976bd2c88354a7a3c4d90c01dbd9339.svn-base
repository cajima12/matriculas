	<name>Seguimientos Electrónicos</name>
	<description>Modulo de procedimientos y seguimientos electrónicos para la administración</description>

	<profiles>
		<!-- DESARROLLO -->
		<profile>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<id>DES</id>
			<properties>
				<build.entorno>Desarrollo</build.entorno>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-war-plugin</artifactId>
						<configuration>
							<webResources>
								<resource>
									<directory>src/main/config/des</directory>
									<targetPath>WEB-INF/classes</targetPath>
								</resource>
							</webResources>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		
		<!-- PRODUCCION -->
		<profile>
			<id>PRO</id>
			<properties>
				<build.entorno>Produccion</build.entorno>
			</properties>

			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-war-plugin</artifactId>
						<configuration>
							<webResources>
								<resource>
									<directory>src/main/config/pro</directory>
									<targetPath>WEB-INF/classes</targetPath>
								</resource>
							</webResources>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>		
	</profiles>

